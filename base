#!/usr/bin/env bash

export DOTFILES_PATH=~/.dotfiles
source $DOTFILES_PATH/aliases
source $DOTFILES_PATH/colors
source $DOTFILES_PATH/osx_prefs
source $DOTFILES_PATH/path
source $DOTFILES_PATH/rbenv

# set editor
export EDITOR='open -a ~/Applications/Sublime\ Text\ 2'

#course stuff
export SKIP_LEVEL_CHECK=true